<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">
  <BehaviorTree ID="Navigation Subtree">
    <RetryUntilSuccessful num_attempts="">
      <Sequence>
        <PathPlan goal_pos="{path_goal}"/>
        <FollowTrajectory/>
      </Sequence>
    </RetryUntilSuccessful>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="FollowTrajectory"
            editable="true"/>
    <Action ID="PathPlan"
            editable="true">
      <input_port name="goal_pos"/>
    </Action>
  </TreeNodesModel>

</root>
