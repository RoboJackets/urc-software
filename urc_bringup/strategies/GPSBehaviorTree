<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4"
      main_tree_to_execute="MainBehavior">
  <BehaviorTree ID="MainBehavior">
    <Sequence>
      <RetryUntilSuccessful num_attempts="5">
        <GetCurrentGoal/>
      </RetryUntilSuccessful>
      <SetBlackboard value="GoalType"
                     output_key="goal_type"/>
      <ScriptCondition code='GoalType == "GPS"'/>
      <IfThenElse>
        <Sequence>
          <SubTree ID="Navigation Subtree"
                   _autoremap="true"/>
          <SearchRadius/>
          <SubTree ID="Navigation Subtree"
                   _autoremap="true"/>
          <RemoveGoal/>
        </Sequence>
        <Sequence>
          <SubTree ID="Navigation Subtree"
                   _autoremap="true"/>
          <RemoveGoal/>
        </Sequence>
      </IfThenElse>
    </Sequence>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="GetCurrentGoal"
            editable="true"/>
    <Action ID="RemoveGoal"
            editable="true"/>
    <Action ID="SearchRadius"
            editable="true"/>
  </TreeNodesModel>

</root>
