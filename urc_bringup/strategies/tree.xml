<root BTCPP_format="4">
  <BehaviorTree ID="MainBehavior">
    <Sequence>
      <RetryUntilSuccessful num_attempts="5">
        <GetCurrentGoal/>
      </RetryUntilSuccessful>
      <SetBlackboard value="GoalType"
                     output_key="goal_type"/>
      <ScriptCondition code='GoalType == "GPS"'/>
      <IfThenElse>
        <Sequence>
          <SubTree ID="Navigation Subtree"
                   _autoremap="true"/>
          <SearchRadius/>
          <SubTree ID="Navigation Subtree"
                   _autoremap="true"/>
          <RemoveGoal/>
        </Sequence>
        <Sequence>
          <SubTree ID="Navigation Subtree"
                   _autoremap="true"/>
          <RemoveGoal/>
        </Sequence>
      </IfThenElse>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="Navigation Subtree">
    <RetryUntilSuccessful num_attempts="">
      <Sequence>
        <PathPlan goal_pos="{path_goal}"/>
        <FollowTrajectory/>
      </Sequence>
    </RetryUntilSuccessful>
  </BehaviorTree>

  <TreeNodesModel>
    <Action ID="GetCurrentGoal"
            editable="true"/>
    <Action ID="RemoveGoal"
            editable="true"/>
    <Action ID="SearchRadius"
      editable="true"/>
    <Action ID="FollowTrajectory"
            editable="true"/>
    <Action ID="PathPlan"
            editable="true">
      <input_port name="goal_pos"/>
    </Action>
  </TreeNodesModel>
</root>
