<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>urc_platform</name>
  <version>0.0.0</version>
  <description>Package for directly controlling rover and joystick control.</description>
  <maintainer email="matthew.scott.hannay@gmail.com">matthewhannay</maintainer>
  <license>MIT</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <test_depend>ament_cmake_gtest<test_depend>
  <test_depend>rclcpp</test_depend>
  <test_depend>rclcpp_components</test_depend>
  <test_depend>urc_msgs</test_depend>
  <test_depend>std_msgs</test_depend>
  <test_depend>nav_msgs</test_depend>
  <test_depend>sensor_msgs</test_depend>
  <test_depend>tf2</test_depend>
  <test_depend>tf2_ros</test_depend>
  <test_depend>laser_geometry</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
